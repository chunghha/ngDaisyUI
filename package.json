{
  "name": "ng-daisyui",
  "version": "0.2.0",
  "engines": {
    "node": ">=18.20.1 <19.0.0",
    "pnpm": ">=9.0.1 <10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "format": "pnpm dlx @biomejs/biome format --write ./src",
    "lint": "pnpm dlx @biomejs/biome lint ./src",
    "lint:fix": "pnpm dlx @biomejs/biome lint --write ./src",
    "check": "pnpm dlx @biomejs/biome check --apply ./src",
    "check:all": "pnpm check && pnpm test:cov"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~18.0.0",
    "@angular/common": "~18.0.0",
    "@angular/compiler": "~18.0.0",
    "@angular/core": "~18.0.0",
    "@angular/forms": "~18.0.0",
    "@angular/platform-browser": "~18.0.0",
    "@angular/platform-browser-dynamic": "~18.0.0",
    "@angular/router": "~18.0.0",
    "@ngneat/elf": "~2.5.1",
    "@tanstack/angular-query-devtools-experimental": "^5.37.1",
    "@tanstack/angular-query-experimental": "^5.37.1",
    "@tanstack/query-core": "^5.36.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.3.1",
    "@analogjs/vite-plugin-angular": "^1.3.1",
    "@angular-devkit/build-angular": "~18.0.0",
    "@angular/cli": "~18.0.0",
    "@angular/compiler-cli": "~18.0.0",
    "@biomejs/biome": "^1.7.3",
    "@fontsource/fira-mono": "^5.0.13",
    "@fontsource/inter": "^5.0.18",
    "@fontsource/poppins": "^5.0.14",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/angular": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.11.1",
    "happy-dom": "^14.11.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5",
    "vitest": "^1.6.0"
  }
}