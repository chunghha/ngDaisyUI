{
  "name": "ng-daisyui",
  "version": "0.3.1",
  "engines": {
    "node": ">=22.18.0 <23.0.0",
    "pnpm": ">=10.14.0 <11.0.0"
  },
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "format": "pnpm dlx @biomejs/biome format --write ./src",
    "lint": "pnpm dlx @biomejs/biome lint ./src",
    "lint:fix": "pnpm dlx @biomejs/biome lint --write ./src",
    "check": "pnpm dlx @biomejs/biome check --write ./src",
    "check:all": "pnpm check && pnpm test:cov"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.2.4",
    "@angular/common": "~20.2.4",
    "@angular/compiler": "~20.2.4",
    "@angular/core": "~20.2.4",
    "@angular/forms": "~20.2.4",
    "@angular/platform-browser": "~20.2.4",
    "@angular/platform-browser-dynamic": "~20.2.4",
    "@angular/router": "~20.2.4",
    "@ngneat/elf": "~2.5.1",
    "@tanstack/angular-query-devtools-experimental": "^5.86.0",
    "@tanstack/angular-query-experimental": "^5.86.0",
    "@tanstack/query-core": "^5.86.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.20.2",
    "@analogjs/vite-plugin-angular": "^1.20.2",
