{
  "name": "ng-daisyui",
  "version": "0.2.0",
  "engines": {
    "node": ">=18.19.0 <19.0.0",
    "pnpm": ">=8.13.1 <9.0.0"
  },
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "format": "pnpm dlx @biomejs/biome format --write ./src",
    "lint": "pnpm dlx @biomejs/biome lint ./src",
    "lint:fix": "pnpm dlx @biomejs/biome lint --write ./src",
    "check": "pnpm dlx @biomejs/biome check --apply ./src",
    "check:all": "pnpm check && pnpm test:cov"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.1.0",
    "@angular/common": "17.1.0",
    "@angular/compiler": "17.1.0",
    "@angular/core": "17.1.0",
    "@angular/forms": "17.1.0",
    "@angular/platform-browser": "17.1.0",
    "@angular/platform-browser-dynamic": "17.1.0",
    "@angular/router": "17.1.0",
    "@ngneat/elf": "~2.4.0",
    "@tanstack/angular-query-devtools-experimental": "^5.17.17",
    "@tanstack/angular-query-experimental": "^5.17.17",
    "@tanstack/query-core": "^5.17.15",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@analogjs/platform": "^0.2.29",
    "@analogjs/vite-plugin-angular": "^0.2.29",
    "@angular-devkit/build-angular": "17.1.0",
    "@angular/cli": "17.1.0",
    "@angular/compiler-cli": "17.1.0",
    "@biomejs/biome": "^1.5.2",
    "@fontsource/fira-mono": "^5.0.8",
    "@fontsource/inter": "^5.0.16",
    "@fontsource/poppins": "^5.0.8",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/angular": "^15.1.0",
    "@types/jest": "^29.5.11",
    "@vitest/coverage-v8": "^1.2.1",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.0",
    "happy-dom": "^13.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vitest": "^1.2.1"
  }
}
