{
  "name": "ng-daisyui",
  "version": "0.2.0",
  "engines": {
    "node": ">=20.17.0 <21.0.0",
    "pnpm": ">=9.0.1 <10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "format": "pnpm dlx @biomejs/biome format --write ./src",
    "lint": "pnpm dlx @biomejs/biome lint ./src",
    "lint:fix": "pnpm dlx @biomejs/biome lint --write ./src",
    "check": "pnpm dlx @biomejs/biome check --write ./src",
    "check:all": "pnpm check && pnpm test:cov"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.0.1",
    "@angular/common": "~19.0.1",
    "@angular/compiler": "~19.0.1",
    "@angular/core": "~19.0.1",
    "@angular/forms": "~19.0.1",
    "@angular/platform-browser": "~19.0.1",
    "@angular/platform-browser-dynamic": "~19.0.1",
    "@angular/router": "~19.0.1",
    "@ngneat/elf": "~2.5.1",
    "@tanstack/angular-query-devtools-experimental": "^5.61.5",
    "@tanstack/angular-query-experimental": "^5.61.5",
    "@tanstack/query-core": "^5.61.5",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.9.4",
    "@analogjs/vite-plugin-angular": "^1.9.4",
    "@angular/build": "^19.0.2",
    "@angular/cli": "~19.0.2",
    "@angular/compiler-cli": "~19.0.1",
    "@biomejs/biome": "^1.9.4",
    "@fontsource/fira-mono": "^5.1.0",
    "@fontsource/inter": "^5.1.0",
    "@fontsource/poppins": "^5.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/angular": "^17.3.2",
    "@types/jest": "^29.5.14",
    "@vitest/coverage-v8": "^2.1.6",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.14",
    "happy-dom": "^15.11.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.3",
    "vitest": "^2.1.6"
  }
