{
  "name": "ng-daisyui",
  "version": "0.3.0",
  "engines": {
    "node": ">=20.17.0 <21.0.0",
    "pnpm": ">=10.0.0 <11.0.0"
  },
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "format": "pnpm dlx @biomejs/biome format --write ./src",
    "lint": "pnpm dlx @biomejs/biome lint ./src",
    "lint:fix": "pnpm dlx @biomejs/biome lint --write ./src",
    "check": "pnpm dlx @biomejs/biome check --write ./src",
    "check:all": "pnpm check && pnpm test:cov"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.0.4",
    "@angular/common": "~20.0.4",
    "@angular/compiler": "~20.0.4",
    "@angular/core": "~20.0.4",
    "@angular/forms": "~20.0.4",
    "@angular/platform-browser": "~20.0.4",
    "@angular/platform-browser-dynamic": "~20.0.4",
    "@angular/router": "~20.0.4",
    "@ngneat/elf": "~2.5.1",
    "@tanstack/angular-query-devtools-experimental": "^5.81.0",
    "@tanstack/angular-query-experimental": "^5.80.10",
    "@tanstack/query-core": "^5.81.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.18.0",
    "@analogjs/vite-plugin-angular": "^1.18.0",
    "@angular/build": "^20.0.3",
    "@angular/cli": "~20.0.3",
    "@angular/compiler-cli": "~20.0.4",
    "@biomejs/biome": "^2.0.4",
    "@fontsource/fira-mono": "^5.2.6",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/poppins": "^5.2.6",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/angular": "^17.4.0",
    "@types/jest": "^29.5.14",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "daisyui": "~5.0.43",
    "happy-dom": "^17.6.3",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4"
  }
